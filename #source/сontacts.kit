<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/slick.css">
    <link rel="stylesheet" href="css/style.css">    


    <title>Womazing</title>
</head>

<body>
    <div class="wrapper">

        <!-- @import "html/header.html" -->
        <!--header-->

    
        <main class="main">

            <section class="contacts">
                <div class="container">

                    <div class="contacts__inner">

                        <div class="contacts__top">
                            <h1 class="contacts__title title-big">Контакти</h1>
                            <div class="bread-crumbs">
                                <a href="index.html">Головна</a><a href="shop.html">Контакти</a>
                            </div>
                        </div>
                        <!-- contacts__top -->

                        <div class="contacts__body">
                            <div class="contacts__map">Карта з будь-якою точкою</div>
                            <!-- contacts__map -->
                            <div class="contacts__info">

                                <div class="contacts__number">
                                    <div class="contacts__number-title">Телефон</div>
                                    <div class="contacts__number-descr">+38 063-534-76-92</div>
                                </div>

                                <div class="contacts__email">
                                    <div class="contacts__email-title">E-mail</div>
                                    <div class="contacts__email-descr">info@sitename.com</div>
                                </div>

                                <div class="contacts__address">
                                    <div class="contacts__address-title">Адреса</div>
                                    <div class="contacts__address-descr">м.~~~ вул.####/23</div>
                                </div>

                            </div>
                            <!-- contacts__info -->

                            <form action="#" class="contacts__form">
                                <div class="contacts-form__title form__title">Напишіть нам</div>

                                <input class="contacts-form__name form__name" name="form__name" type="text"
                                    placeholder="Ім'я" tabindex="1">

                                <input class="contacts-form__email form__email" name="form__email" type="email"
                                    placeholder="E-mail" tabindex="2">

                                <input class="contacts-form__number form__number" name="form__number" type="tel"
                                    placeholder="Телефон" tabindex="3">

                                <textarea class="contacts-form__mess form__mess" name="form__mess"
                                    placeholder="Повідомлення" tabindex="4"></textarea>

                                <button class="contacts-form__btn btn" type="submit" tabindex="5">Надіслати</button>

                                <div class="contacts-form__info form__info">Повідомлення успішно відправлено</div>
                            </form>
                            <!-- contacts__form -->
                        </div>
                        <!-- contacts__body -->
                    </div>
                    <!-- contacts__inner -->
                </div>
                <!-- container -->
            </section>
            <!-- contacts -->
        </main>


        <!-- @import "html/footer.html" -->
        <!--footer-->  

    </div>
<!--wrapper-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/main.js"></script>
</body>

</html>