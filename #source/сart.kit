<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/slick.css">
    <link rel="stylesheet" href="css/style.css">    


    <title>Womazing</title>
</head>

<body>
    <div class="wrapper">

        <!-- @import "html/header.html" -->
        <!--header-->



        <main class="main">
            <section class="cart">
                <div class="container">
                    <div class="cart__inner">

                        <div class="cart__top">
                            <h1 class="cart__title title-big">Кошик</h1>
                            <div class="bread-crumbs">
                                <a href="index.html">Головна</a><a href="shop.html">Кошик</a>
                            </div>
                        </div>
                        <!-- cart__top -->
                        <div class="cart__body">
                            <div class="cart__table">


                                <div class="cart-table__row">
                                    <div class="cart-table__col">Товар</div>
                                    <div class="cart-table__col">Ціна</div>
                                    <div class="cart-table__col">Кількість</div>
                                    <div class="cart-table__col">Усього</div>
                                </div>
                                <div class="cart-table__row">
                                    <div class="cart-table__col card-product">
                                        <img src="img/collection-img1.png" alt="">
                                        Футболка USA
                                    </div>
                                    <div class="cart-table__col cart-price1">$129</div>
                                    <div class="cart-table__col card-btn">1</div>
                                    <div class="cart-table__col cart-price2">$129</div>
                                </div>

                            </div>
                            <!-- cart__table -->
                            <form action="#" class="cart__form">
                                <div class="cart__form-box">
                                    <div class="cart__form-input-box">
                                        <input type="text" class="cart__enter-coupon form__input-style"
                                            placeholder="Введіть купон">
                                        <button type="submit" class="cart__btn-cupon btn1">Застосувати купон</button>
                                    </div>
                                    <button type="reset" class="cart__btn-restart btn1">Оновити кошик</button>
                                </div>
                                <!-- cart__form-box -->
                                <div class="cart__result-box">
                                    <div class="cart__result-text1">Підсумок: $129</div>
                                    <div class="cart__result-text-box">

                                        <div class="cart__result-text2">Разом: <span>$129</span></div>
                                        <button type="submit" class="cart__result-btn-cupon btn">Оформити
                                            замовлення</button>
                                    </div>
                                    <!-- cart__result-text-box -->
                                </div>
                                <!-- cart__result-box -->
                            </form>
                            <!-- cart__form -->
                        </div>
                        <!-- cart__body -->
                    </div>
                    <!-- cart__inner -->
                </div>
<!-- container -->
            </section>
            <!-- cart -->
        </main>


        <!-- @import "html/footer.html" -->
        <!--footer-->  

    </div>
<!--wrapper-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/main.js"></script>
</body>

</html>